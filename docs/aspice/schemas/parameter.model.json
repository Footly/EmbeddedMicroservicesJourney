{
    "$schema": "./codearchitect.schema.json",
    "type": "object",
    "title": "Parameter",
    "properties": {
        "id": {
            "type": "string",
            "hidden": true
        },
        "label": {
            "type": "string",
            "hidden": true
        },
        "visibility": {
            "$ref": "./visibility.model.json",
            "hidden": true,
            "editable": true
        },
        "tags": {
            "hidden": true,
            "type": "array",
            "items": {
                "type": "string"
            },
            "const": [
                "parameter"
            ]
        },
        "icon": {
            "hidden": true,
            "type": "string",
            "const": "symbol-parameter"
        },
        "datatype": {
            "editable": true,
            "hidden": true,
            "type": "string",
            "search": {
                "query": [
                    [
                        {
                            "field": "tags",
                            "regex": "type"
                        }
                    ]
                ]
            }
        },
        "Array": {
            "hidden": true,
            "type": "array",
            "editable": true,
            "items": {
                "type": "string",
                "search": {
                    "query": [
                        [
                            {
                                "field": "tags",
                                "regex": "enummember"
                            }
                        ]
                    ],
                    "text": true
                }
            }
        },
        "qualifiers and pointers": {
            "type": "string",
            "hidden": true,
            "editable": true
        },
        "array": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "hidden": true,
            "editable": true
        }
    }
}