{
    "$schema": "./codearchitect.schema.json",
    "type": "object",
    "title": "Enum",
    "properties": {
        "id": {
            "type": "string",
            "hidden": true
        },
        "label": {
            "type": "string",
            "hidden": true
        },
        "tags": {
            "hidden": true,
            "type": "array",
            "items": {
                "type": "string"
            },
            "const": [
                "type",
                "enumerator"
            ]
        },
        "visibility": {
            "$ref": "./visibility.model.json",
            "hidden": true,
            "editable": true
        },
        "icon": {
            "hidden": true,
            "type": "string",
            "const": "list-ordered"
        },
        "items": {
            "type": "array",
            "hidden": true,
            "items": {
                "$ref": "./enummember.model.json"
            }
        },
        "description": {
            "hidden": true,
            "editable": true,
            "multiline": true,
            "type": "string"
        }
    }
}